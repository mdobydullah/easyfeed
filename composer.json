{
    "name": "obydul/easyfeed",
    "description": "EasyFeed is a simple PHP package to create and read RSS & Atom feed specially for Laravel.",
    "keywords": [
        "laravel feed reader", "laravel create rss feed", "php feed reader", "php feed creator", "RSS feed parser",
        "laravel feed parser", "php feed parser", "laravel atom feed", "laravel feed generator", "php feed generator"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Obydullah",
            "email": "obydul@makpie.com"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Obydul\\EasyFeed\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Obydul\\EasyFeed\\FeedReaderServiceProvider"
            ],
            "aliases": {
                "FeedReader": "Obydul\\EasyFeed\\Facades\\FeedReader"
            }
        }
    }
}
